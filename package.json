{
  "name": "storybook-icon",
  "displayName": "Storybook Icon",
  "icon": "icons/icon.png",
  "publisher": "KarimAmouri",
  "version": "1.1.0",
  "description": "a VS code extension that changes icon for storybook files",
  "categories": [
    "Other"
  ],
  "scripts": {
    "build": "vsce package"
  },
  "contributes": {
    "languages": [
      {
        "id": "stories",
        "aliases": [
          "Stories"
        ],
        "extensions": [
          ".stories.ts",
          ".stories.js"
        ],
        "icon": {
          "light": "./icons/icon.svg",
          "dark": "./icons/icon.svg"
        }
      }
    ]
  },
  "engines": {
    "vscode": "^1.103.0"
  },
  "author": "Karim Amouri",
  "license": "MIT"
}
